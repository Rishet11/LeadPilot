# LeadPilot Frontend Environment Variables
# Copy this to .env.local and fill in your values

# API URL (your backend server)
# Local development:
NEXT_PUBLIC_API_URL=http://localhost:8000

# Production (uncomment and set):
# NEXT_PUBLIC_API_URL=https://your-api-server.com

# Google OAuth client ID for Sign in with Google
NEXT_PUBLIC_GOOGLE_CLIENT_ID=

# Optional: Dodo Payments checkout URLs for pricing page
NEXT_PUBLIC_DODO_LAUNCH_URL=
NEXT_PUBLIC_DODO_STARTER_URL=
